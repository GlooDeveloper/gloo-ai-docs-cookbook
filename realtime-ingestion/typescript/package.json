    "single": "npm run dev single ./sample_content/developer_happiness.txt"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  },
  "keywords": [
    "gloo-ai",
    "content-ingestion",
    "realtime-api",
    "typescript"
  ],
