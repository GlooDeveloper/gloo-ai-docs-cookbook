{
  "name": "realtime-ingestion-typescript",
  "version": "1.0.0",
  "description": "TypeScript example for Gloo AI Realtime Content Ingestion API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "npx ts-node index.ts",
